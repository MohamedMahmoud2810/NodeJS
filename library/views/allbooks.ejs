<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <form >
        <input type="text" name="q" value="<%=q %>" /><input type="submit" value="search"></br>
    </form>
    <table>
        <tr>
            <th>Code</th>
            <th>Name</th>
            <th>Author</th>
            <th>Action</th>
        </tr>
        <% for (let book of fbooks ) { %>
        <tr>
            <td><%= book.code %></td>
            <td><%= book.name %></td>
            <td><%= book.author %></td>
            <td>
                <a href="/updatebook?code=<%= book.code %>" >Edit</a> 
                <a href="/deletebook?code=<%= book.code %>" >Delete</a>
            </td>
        </tr>
        <% } %>
    </table>
    <h1>Add book</h1>
    <form action="/allbooks" method="POST">
        Name:<input type="text" name="name" /><br/>
        Author:<input type="text" name="author" /><br/>
        <input type="submit" value="add" />
    </form>

    
</body>
</html>